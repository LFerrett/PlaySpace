<!-- Creates a large card in materialize -->
<div class="card medium">
  <div class="card-image">
    <img src="{{image}}" />
    <span class="card-title"><i class="material-icons">playlist_play</i> {{name}}</span>
    <div class="right">
        <a href="#"><i class="material-icons icon-blue">star_border</i></a>
    </div>
  </div>
  <div class="card-content">
    <p>Playlist Data Goes Here</p>
  </div>
  <div class="card-action .left-align">
    <a href="#"><i class="material-icons">thumb_up</i> {{upvotes}}</a>
    <a href="#"><i class="material-icons">comment</i> {{comments}}</a>
  </div>
</div>